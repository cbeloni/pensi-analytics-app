pyinstaller --windowed --hidden-import=xgboost --hidden-import=imblearn --collect-all xgboost --collect-submodules imblearn main.py

pyinstaller --windowed --hidden-import=xgboost --hidden-import=imblearn --hidden-import=statsmodels --collect-all xgboost --collect-submodules imblearn --collect-all statsmodels main.py

pyinstaller --windowed --hidden-import=xgboost --hidden-import=imblearn --hidden-import=statsmodels --hidden-import=yaml --collect-all xgboost --collect-submodules imblearn --collect-all statsmodels main.py

pyinstaller --onefile --hidden-import=xgboost --hidden-import=imblearn --hidden-import=statsmodels --hidden-import=yaml --collect-all xgboost --collect-submodules imblearn --collect-all statsmodels main.py